[display]

window/size/viewport_width=960
window/size/viewport_height=540
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[dotnet]

project/assembly_name="Balls Balls Balls"

[gui]

theme/custom_font="res://fonts/Iceland-Regular.ttf"

[layer_names]

2d_physics/layer_1="Balls"
2d_physics/layer_2="Paddles"
2d_physics/layer_3="Goals"
2d_physics/layer_4="Bumpers"
2d_physics/layer_5="Walls"
